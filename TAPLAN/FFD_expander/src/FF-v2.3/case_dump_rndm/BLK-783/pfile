(define  (problem zeno11_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	plane1 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
	person6 - person
	person7 - person
	person8 - person
	person9 - person
	person10 - person
)
(:init
 	(at plane1 city3)
	(at person1 city3)
	(at person2 city2)
	(at person3 city1)
	(at person4 city1)
	(at person5 city3)
	(at person6 city3)
	(at person7 city6)
	(at person8 city5)
	(at person9 city1)
	(at person10 city3)
	(fuel-level plane1 fl3)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city3)
	(at person1 city5)
	(at person2 city5)
	(at person3 city4)
	(at person4 city1)
	(at person5 city4)
	(at person6 city1)
	(at person7 city1)
	(at person8 city2)
	(at person9 city6)
)))
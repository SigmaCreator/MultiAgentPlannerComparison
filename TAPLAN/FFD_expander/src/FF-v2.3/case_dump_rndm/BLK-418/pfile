	person6 - person
	person7 - person
	person8 - person
	person9 - person
	person10 - person
)
(:init
 	(at plane1 city1)
	(at plane2 city7)
	(at plane3 city4)
	(at plane4 city7)
	(at plane5 city1)
	(at plane6 city1)
	(at plane7 city2)
	(at plane8 city5)
	(at plane9 city7)
	(at person1 city2)
	(at person2 city3)
	(at person3 city3)
	(at person4 city7)

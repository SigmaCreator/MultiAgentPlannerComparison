 	(at plane1 city2)
	(at plane2 city1)
	(at plane3 city1)
	(at plane4 city7)
	(at plane5 city2)
	(at plane6 city6)
	(at plane7 city6)
	(at plane8 city7)
	(at plane9 city3)
	(at plane10 city5)
	(at person1 city1)
	(at person2 city2)
	(at person3 city1)
	(fuel-level plane1 fl1)
	(fuel-level plane2 fl2)
	(fuel-level plane3 fl5)
	(fuel-level plane4 fl5)
	(fuel-level plane5 fl4)
	(fuel-level plane6 fl4)
	(fuel-level plane7 fl0)
	(fuel-level plane8 fl2)
	(fuel-level plane9 fl5)
	(fuel-level plane10 fl5)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city6)
	(at plane2 city4)
	(at plane3 city8)
	(at plane4 city5)
	(at plane5 city8)
	(at plane6 city8)
	(at plane7 city6)

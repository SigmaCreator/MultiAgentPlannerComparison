(define  (problem zeno250_2)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	city8 - city
	city9 - city
	city10 - city
	city11 - city
	city12 - city
	city13 - city
	city14 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	person1 - person
)
(:init
 	(at plane1 city6)
	(at plane2 city3)
	(at plane3 city14)
	(at plane4 city9)
	(at person1 city12)
	(fuel-level plane1 fl6)
	(fuel-level plane2 fl4)
	(fuel-level plane3 fl4)
	(fuel-level plane4 fl6)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city13)
	(at plane2 city11)
	(at plane3 city12)
	(at person1 city13)
)))
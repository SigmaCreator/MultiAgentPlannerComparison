(define  (problem zeno104_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	city8 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
	person6 - person
	person7 - person
	person8 - person
)
(:init
 	(at plane1 city6)
	(at plane2 city2)
	(at plane3 city4)
	(at plane4 city4)
	(at plane5 city6)
	(at plane6 city4)
	(at plane7 city5)
	(at person1 city8)
	(at person2 city8)
	(at person3 city3)
	(at person4 city8)
	(at person5 city5)
	(at person6 city6)
	(at person7 city7)
	(at person8 city5)
	(fuel-level plane1 fl5)
	(fuel-level plane2 fl3)
	(fuel-level plane3 fl5)
	(fuel-level plane4 fl4)
	(fuel-level plane5 fl0)
	(fuel-level plane6 fl0)
	(fuel-level plane7 fl2)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city7)
	(at plane2 city4)
	(at plane3 city7)
	(at person1 city4)
	(at person2 city5)
	(at person3 city7)
	(at person4 city4)
	(at person5 city1)
	(at person6 city2)
	(at person7 city8)
)))
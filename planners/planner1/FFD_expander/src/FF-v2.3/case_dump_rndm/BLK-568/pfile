(define  (problem zeno66_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	city8 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	person1 - person
	person2 - person
)
(:init
 	(at plane1 city5)
	(at plane2 city8)
	(at plane3 city8)
	(at plane4 city5)
	(at plane5 city6)
	(at plane6 city6)
	(at person1 city8)
	(at person2 city7)
	(fuel-level plane1 fl3)
	(fuel-level plane2 fl5)
	(fuel-level plane3 fl3)
	(fuel-level plane4 fl6)
	(fuel-level plane5 fl1)
	(fuel-level plane6 fl4)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city5)
	(at plane2 city6)
	(at plane3 city8)
	(at plane4 city2)
	(at plane5 city4)
	(at plane6 city8)
	(at person1 city7)
)))
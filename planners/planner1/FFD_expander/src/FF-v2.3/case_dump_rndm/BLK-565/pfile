(define  (problem zeno227_4)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	plane8 - aircraft
	plane9 - aircraft
	plane10 - aircraft
	plane11 - aircraft
	person1 - person
)
(:init
 	(at plane1 city1)
	(at plane2 city2)
	(at plane3 city2)
	(at plane4 city2)
	(at plane5 city1)
	(at plane6 city2)
	(at plane7 city2)
	(at plane8 city2)
	(at plane9 city1)
	(at plane10 city2)
	(at plane11 city2)
	(at person1 city2)
	(fuel-level plane1 fl0)
	(fuel-level plane2 fl0)
	(fuel-level plane3 fl1)
	(fuel-level plane4 fl1)
	(fuel-level plane5 fl2)
	(fuel-level plane6 fl5)
	(fuel-level plane7 fl5)
	(fuel-level plane8 fl1)
	(fuel-level plane9 fl6)
	(fuel-level plane10 fl0)
	(fuel-level plane11 fl3)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city2)
	(at person1 city2)
)))
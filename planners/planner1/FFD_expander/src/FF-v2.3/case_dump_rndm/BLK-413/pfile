(define  (problem zeno109_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	plane1 - aircraft
	plane2 - aircraft
	person1 - person
)
(:init
 	(at plane1 city4)
	(at plane2 city3)
	(at person1 city4)
	(fuel-level plane1 fl1)
	(fuel-level plane2 fl1)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city3)
	(at plane2 city1)
	(at person1 city4)
)))
(define  (problem zeno113_4)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
	person6 - person
	person7 - person
)
(:init
 	(at plane1 city3)
	(at plane2 city1)
	(at plane3 city1)
	(at plane4 city2)
	(at plane5 city2)
	(at plane6 city4)
	(at plane7 city2)
	(at person1 city3)
	(at person2 city4)
	(at person3 city3)
	(at person4 city1)
	(at person5 city2)
	(at person6 city3)
	(at person7 city2)
	(fuel-level plane1 fl0)
	(fuel-level plane2 fl1)
	(fuel-level plane3 fl2)
	(fuel-level plane4 fl2)
	(fuel-level plane5 fl5)
	(fuel-level plane6 fl4)
	(fuel-level plane7 fl0)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city1)
	(at plane2 city4)
	(at person1 city3)
)))
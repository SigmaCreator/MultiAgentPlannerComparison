(define  (problem zeno83_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
)
(:init
 	(at plane1 city4)
	(at plane2 city1)
	(at plane3 city4)
	(at plane4 city2)
	(at plane5 city7)
	(at plane6 city2)
	(at plane7 city6)
	(at person1 city7)
	(at person2 city2)
	(at person3 city6)
	(at person4 city3)
	(at person5 city1)
	(fuel-level plane1 fl2)
	(fuel-level plane2 fl5)
	(fuel-level plane3 fl3)
	(fuel-level plane4 fl3)
	(fuel-level plane5 fl1)
	(fuel-level plane6 fl3)
	(fuel-level plane7 fl2)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city2)
	(at plane2 city1)
	(at person1 city1)
	(at person2 city2)
	(at person3 city1)
	(at person4 city6)
	(at person5 city1)
)))
(define  (problem zeno61_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	city8 - city
	city9 - city
	city10 - city
	plane1 - aircraft
	plane2 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
	person6 - person
	person7 - person
	person8 - person
)
(:init
 	(at plane1 city6)
	(at plane2 city1)
	(at person1 city10)
	(at person2 city1)
	(at person3 city4)
	(at person4 city9)
	(at person5 city5)
	(at person6 city2)
	(at person7 city9)
	(at person8 city9)
	(fuel-level plane1 fl0)
	(fuel-level plane2 fl6)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city2)
	(at plane2 city4)
	(at person1 city7)
	(at person2 city3)
	(at person3 city3)
	(at person4 city6)
	(at person5 city1)
	(at person6 city5)
)))
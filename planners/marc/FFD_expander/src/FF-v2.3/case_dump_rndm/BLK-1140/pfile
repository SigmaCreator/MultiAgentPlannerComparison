(define  (problem zeno174_2)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	city8 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	plane8 - aircraft
	person1 - person
	person2 - person
)
(:init
 	(at plane1 city7)
	(at plane2 city3)
	(at plane3 city3)
	(at plane4 city3)
	(at plane5 city3)
	(at plane6 city6)
	(at plane7 city4)
	(at plane8 city1)
	(at person1 city7)
	(at person2 city4)
	(fuel-level plane1 fl2)
	(fuel-level plane2 fl3)
	(fuel-level plane3 fl2)
	(fuel-level plane4 fl5)
	(fuel-level plane5 fl2)
	(fuel-level plane6 fl3)
	(fuel-level plane7 fl5)
	(fuel-level plane8 fl2)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city1)
	(at person1 city6)
)))
(define  (problem zeno171_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	city3 - city
	city4 - city
	city5 - city
	city6 - city
	city7 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	person1 - person
	person2 - person
	person3 - person
	person4 - person
	person5 - person
	person6 - person
	person7 - person
	person8 - person
	person9 - person
	person10 - person
)
(:init
 	(at plane1 city1)
	(at plane2 city3)
	(at plane3 city5)
	(at plane4 city6)
	(at plane5 city5)
	(at person1 city2)
	(at person2 city7)
	(at person3 city6)
	(at person4 city4)
	(at person5 city1)
	(at person6 city4)
	(at person7 city2)
	(at person8 city6)
	(at person9 city5)
	(at person10 city3)
	(fuel-level plane1 fl3)
	(fuel-level plane2 fl4)
	(fuel-level plane3 fl6)
	(fuel-level plane4 fl4)
	(fuel-level plane5 fl3)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city3)
	(at plane2 city4)
	(at plane3 city2)
	(at plane4 city1)
	(at plane5 city3)
	(at person1 city7)
	(at person2 city6)
	(at person3 city1)
)))
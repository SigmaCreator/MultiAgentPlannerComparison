(define  (problem zeno246_1)
(:domain zeno-travel)
(:objects
 	fl0 - flevel
	fl1 - flevel
	fl2 - flevel
	fl3 - flevel
	fl4 - flevel
	fl5 - flevel
	fl6 - flevel
	city1 - city
	city2 - city
	plane1 - aircraft
	plane2 - aircraft
	plane3 - aircraft
	plane4 - aircraft
	plane5 - aircraft
	plane6 - aircraft
	plane7 - aircraft
	plane8 - aircraft
	plane9 - aircraft
	person1 - person
	person2 - person
)
(:init
 	(at plane1 city1)
	(at plane2 city2)
	(at plane3 city2)
	(at plane4 city1)
	(at plane5 city1)
	(at plane6 city1)
	(at plane7 city2)
	(at plane8 city2)
	(at plane9 city2)
	(at person1 city1)
	(at person2 city2)
	(fuel-level plane1 fl3)
	(fuel-level plane2 fl2)
	(fuel-level plane3 fl6)
	(fuel-level plane4 fl4)
	(fuel-level plane5 fl3)
	(fuel-level plane6 fl1)
	(fuel-level plane7 fl5)
	(fuel-level plane8 fl6)
	(fuel-level plane9 fl1)
	(next fl0 fl1)
	(next fl1 fl2)
	(next fl2 fl3)
	(next fl3 fl4)
	(next fl4 fl5)
	(next fl5 fl6))
(:goal (and 	(at plane1 city2)
	(at plane2 city2)
	(at plane3 city1)
	(at plane4 city2)
	(at plane5 city2)
	(at plane6 city2)
	(at plane7 city2)
	(at person1 city2)
	(at person2 city1)
)))
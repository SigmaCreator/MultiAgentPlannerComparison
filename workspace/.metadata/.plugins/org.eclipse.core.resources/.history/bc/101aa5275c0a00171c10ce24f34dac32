			boolean print = false;
			PrintWriter writer = new PrintWriter("/home/rovane/planners/results/r1/adp/cost/p01.out", "UTF-8");
		    for(String line; (line = br.readLine()) != null; ) {
		    	if (line.contains("sys")) {
		    		print = true;
		    	}
		    	if (line.contains("(") && print && !line.contains(";")) 
	    			writer.println(line);
		        // process the line.
		    }
		    writer.close();
		    // line is not visible here.
		}
		
